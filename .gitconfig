[user]
    name = Kent B. Larsen
    email = kent.b.larsen@gmail.com
[core]
    editor = vim
    excludesfile = ~/.gitignore
    autocrlf = input
[merge]
    tool = vimdiff
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
[color]
    ui = true
[push]
    default = simple
